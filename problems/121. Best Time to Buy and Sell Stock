class Solution {
    public int maxProfit(int[] prices) {
            int i=0 ;
            int maxProfit = 0;
            for(int j=1 ; j<prices.length ; j++)
            {
                if(prices[j]>prices[i])
                {
                    maxProfit = Math.max((prices[j] - prices[i]) , maxProfit) ; 
                }
                else{
                    i++;
                }
            }
        return maxProfit ; 
    }
}
